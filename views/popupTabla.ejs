<body>

    <div id="overlay"></div>

    <div id="popup2">
        <div class="popup-container">
            <button class="cerrar" onclick="cerrarPopup()"><i id="closer" class="fa-solid fa-xmark"></i></button>
            <h1 id="titulo">Crear tabla</h1>
            <form action="/crearTabla" method="post">
                <label for="nombre">Nombre de la tabla:</label>
                <input id="username" type="text" placeholder="Nombre de la tabla..." name="nombre">
                <div class="boton2">
                    <input id="boton2" type="submit" class="btn-save" value="Crear"></input>
                </div>
            </form>
            
        </div>
        
    </div>

<script src="/javascripts/popupTabla.js"></script>
